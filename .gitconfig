[user]
    email = d.maylarov@emzior.ru
    name = Dmitry Maylarov
	signingkey = E9D4CC42C8F4A442811ABD7DB0FF4856255CC2CA
[core]
	autocrlf = false
	safecrlf = false
    excludesfile = ~/.gitignore
[alias]
    co = checkout
    ci = commit
    st = status
    br = branch
    lg = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
    type = cat-file -t
    dump = cat-file -p
	hist = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
[credential]
    helper = cache --timeout=7200
[gpg]
	program = gpg
[url "git@github.com:"]
	insteadOf = https://github.com/
[commit]
	gpgsign = false
